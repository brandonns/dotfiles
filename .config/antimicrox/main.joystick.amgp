<?xml version="1.0" encoding="UTF-8"?>
<joystick configversion="19" appversion="3.1.4">
    <!--The SDL name for a joystick is included for informational purposes only.-->
    <sdlname>ClockworkPI uConsole</sdlname>
    <!--The Unique ID for a joystick is included for informational purposes only.-->
    <uniqueID>03000000af1e00002400000010010000</uniqueID>
    <profilename>main</profilename>
    <sets>
        <set index="1">
            <axis index="2">
                <center_value>0</center_value>
                <min_value>-32767</min_value>
                <max_value>32767</max_value>
                <throttle>normal</throttle>
            </axis>
            <axis index="1">
                <center_value>0</center_value>
                <min_value>-32767</min_value>
                <max_value>32767</max_value>
                <throttle>normal</throttle>
            </axis>
            <axis index="4">
                <center_value>0</center_value>
                <min_value>-32767</min_value>
                <max_value>32767</max_value>
                <throttle>normal</throttle>
            </axis>
            <axis index="3">
                <center_value>0</center_value>
                <min_value>-32767</min_value>
                <max_value>32767</max_value>
                <throttle>normal</throttle>
            </axis>
            <axis index="5">
                <center_value>0</center_value>
                <min_value>-32767</min_value>
                <max_value>32767</max_value>
                <throttle>normal</throttle>
            </axis>
            <button index="2">
                <slots>
                    <slot>
                        <slots>
                            <slot>
                                <code>0x11000021</code>
                                <mode>keyboard</mode>
                            </slot>
                            <slot>
                                <code>0x11000020</code>
                                <mode>keyboard</mode>
                            </slot>
                        </slots>
                        <mode>mix</mode>
                    </slot>
                </slots>
            </button>
            <button index="1">
                <actionname>copy</actionname>
                <slots>
                    <slot>
                        <code>0x11000022</code>
                        <mode>keyboard</mode>
                    </slot>
                    <slot>
                        <path>/home/$USER/$PATH/scripts/uniCopy.sh</path>
                        <mode>execute</mode>
                    </slot>
                </slots>
            </button>
            <button index="4">
                <slots>
                    <slot>
                        <code>0x1000023</code>
                        <mode>keyboard</mode>
                    </slot>
                </slots>
            </button>
            <button index="3">
                <slots>
                    <slot>
                        <slots>
                            <slot>
                                <code>0x1000023</code>
                                <mode>keyboard</mode>
                            </slot>
                            <slot>
                                <code>0x1000020</code>
                                <mode>keyboard</mode>
                            </slot>
                        </slots>
                        <mode>mix</mode>
                    </slot>
                </slots>
            </button>
            <button index="10">
                <slots>
                    <slot>
                        <code>0x11000022</code>
                        <mode>keyboard</mode>
                    </slot>
                    <slot>
                        <path>/usr/bin/i3lock</path>
                        <arguments>--image /home/$USER/$PATH/locks/orbsPills.png</arguments>
                        <mode>execute</mode>
                    </slot>
                </slots>
            </button>
            <button index="9">
                <slots>
                    <slot>
                        <code>0x11000022</code>
                        <mode>keyboard</mode>
                    </slot>
                    <slot>
                        <path>/home/$USER/$PATH/scripts/llm.sh</path>
                        <mode>execute</mode>
                    </slot>
                </slots>
            </button>
        </set>
    </sets>
